      "slate-900": "#0f172a"
    },
    "gradients": {
      "blue-cyan": "linear-gradient(to right, #3b82f6, #06b6d4)",
      "blue-purple": "linear-gradient(to right, #3b82f6, #8b5cf6)",
      "pink-rose": "linear-gradient(to right, #ec4899, #f43f5e)",
      "green-emerald": "linear-gradient(to right, #22c55e, #10b981)",
      "purple-indigo": "linear-gradient(to right, #8b5cf6, #6366f1)"
    },
    "backgrounds": {
      "primary": "#ffffff",
      "secondary": "#f8fafc",
      "accent": "#eff6ff"
    }
  },
  "typography": {
    "fontFamily": {
      "primary": "Inter, system-ui, sans-serif",
      "display": "Lexend, system-ui, sans-serif"
    },
    "fontSize": {
      "xs": "12px",
      "sm": "14px",
      "base": "16px",
      "lg": "18px",
      "xl": "20px",
      "2xl": "24px",
      "3xl": "30px",
      "4xl": "36px",
      "5xl": "48px",
      "6xl": "60px"
    },
    "fontWeight": {
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700"
    },
    "lineHeight": {
      "tight": "1.25",
      "normal": "1.5",
      "relaxed": "1.625"
    }
  },
  "spacing": {
    "px": "1px",
    "0.5": "2px",
    "1": "4px",
    "1.5": "6px",
    "2": "8px",
    "3": "12px",
    "4": "16px",
    "5": "20px",
    "6": "24px",
    "8": "32px",
    "10": "40px",
    "12": "48px",
    "16": "64px",
    "20": "80px",
    "24": "96px",
    "32": "128px"
  },
  "borderRadius": {
    "sm": "4px",
    "md": "6px",
    "lg": "8px",
    "xl": "12px",
    "2xl": "16px",
    "3xl": "24px",
    "full": "9999px"
  },
  "shadows": {
    "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
    "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
    "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
    "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
    "2xl": "0 25px 50px -12px rgb(0 0 0 / 0.25)"
  },
  "animations": {
    "blob": {
      "name": "blob",
      "duration": "7s",
      "iterationCount": "infinite",
      "keyframes": {
        "0%": "translate(0px, 0px) scale(1)",
        "33%": "translate(30px, -50px) scale(1.1)",
        "66%": "translate(-20px, 20px) scale(0.9)",
        "100%": "translate(0px, 0px) scale(1)"
      }
    },
    "fade-in": {
      "name": "fade-in",
      "duration": "0.4s",
      "timingFunction": "ease-out",
      "keyframes": {
        "0%": "opacity: 0, transform: translateY(10px)",
        "100%": "opacity: 1, transform: translateY(0)"
      }
    },
    "scale-in": {
      "name": "scale-in",
      "duration": "0.3s",
      "timingFunction": "ease-out",
      "keyframes": {
        "0%": "transform: scale(0.95), opacity: 0",
        "100%": "transform: scale(1), opacity: 1"
      }
    }
  }
}
